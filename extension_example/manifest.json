{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "My extension",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.3.min.js","content.js"]
    }
  ],
  "background": {
    "scripts":["background.js"]
  } ,

  "browser_action": {
    "default_icon": "icon.png"

  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs"

  ]
}